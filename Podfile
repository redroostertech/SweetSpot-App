pod 'Alamofire'
pod 'AWSS3'
pod 'SwiftyJSON'

  target 'SweetSpotTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SweetSpotUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
